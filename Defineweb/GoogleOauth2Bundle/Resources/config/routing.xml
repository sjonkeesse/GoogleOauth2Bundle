<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="google_oauth2_refresh_access_token" path="/renew">
        <default key="_controller">GoogleOauth2Bundle:OAuth:renewAccessToken</default>
    </route>

    <route id="google_oauth2_callback" path="/callback">
        <default key="_controller">GoogleOauth2Bundle:Redirect:callback</default>
    </route>
</routes>
